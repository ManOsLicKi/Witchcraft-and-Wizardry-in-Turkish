# fill lader
execute as @s[scores={height=1}] at @s run fill ~3 ~0 ~3 ~-3 ~0 ~-3 air replace ladder
execute as @s[scores={height=1}] at @s run fill ~2 ~0 ~2 ~-2 ~0 ~-2 air replace clay
execute at @s[scores={height=1}] run fill ~1 ~0 ~1 ~-1 ~0 ~-1 clay

execute as @s[scores={height=2}] at @s run fill ~3 ~1 ~3 ~-3 ~0 ~-3 air replace ladder
execute as @s[scores={height=2}] at @s run fill ~2 ~1 ~2 ~-2 ~0 ~-2 air replace clay
execute at @s[scores={height=2}] run fill ~1 ~0 ~1 ~-1 ~1 ~-1 clay
execute at @s[scores={height=2}] run fill ~-2 ~1 ~0 ~-2 ~1 ~0 minecraft:ladder[facing=west,waterlogged=false]

